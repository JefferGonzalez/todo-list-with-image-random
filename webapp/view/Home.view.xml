<mvc:View
	controllerName="todolist.controller.Home"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true"
	xmlns="sap.m"
>
	<Page
		id="page"
		title="{i18n>appTitle}"
	>
		<Text
			id="_IDGenText1"
			text="home"
			textAlign="Begin"
			textDirection="Inherit"
			width=""
			wrapping="true"
			wrappingType="Normal"
		/>
		<Button
			id="_IDGenButton1"
			text="Go to target view"
			press=".onDisplayNotFound"
		/>
		<mvc:XMLView
			id="activitiesView"
			viewName="todolist.view.ActivitiesList"
		/>
	</Page>
</mvc:View>